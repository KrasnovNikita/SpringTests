<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-lazy-init="true">


	<bean id="t100" class="ua.krasnovnikita.robotimpl.robot.ModelT100"
		p:hand-ref="sonyHand" p:head-ref="toshibaHead" p:leg-ref="sonyLeg" scope="prototype" 
		>


		<constructor-arg value="silver" index="0" type="String"></constructor-arg>
		<constructor-arg value="1991" index="1" type="int"></constructor-arg>
		<constructor-arg value="true" index="2" type="boolean"></constructor-arg>



	</bean>

	<bean id="t100Empty" class="ua.krasnovnikita.robotimpl.robot.ModelT100"
		c:color="gold" c:year="2001" c:soundEnabled="true" >


		<property name="hand" ref="sonyHand"></property>
		<property name="head" ref="sonyHead"></property>
		<property name="leg" ref="sonyLeg"></property>



	</bean>
	<bean id="sonyHand" class="ua.krasnovnikita.robotimpl.sony.SonyHand">
	</bean>
	<bean id="sonyHead" class="ua.krasnovnikita.robotimpl.sony.SonyHead">
	</bean>
	<bean id="sonyLeg" class="ua.krasnovnikita.robotimpl.sony.SonyLeg">
	</bean>

	<bean id="toshibaHand" class="ua.krasnovnikita.robotimpl.toshiba.ToshibaHand">
	</bean>
	<bean id="toshibaHead" class="ua.krasnovnikita.robotimpl.toshiba.ToshibaHead">
	</bean>
	<bean id="toshibaLeg" class="ua.krasnovnikita.robotimpl.toshiba.ToshibaLeg">
	</bean>
	
	<bean class="ua.krasnovnikita.robotimpl.start.MyBeanPostProcessor"/>
</beans>
